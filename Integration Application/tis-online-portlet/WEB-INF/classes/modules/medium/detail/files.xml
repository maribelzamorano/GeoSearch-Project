<struts>
	<package name="medium_detail_files" extends="topdev-default" namespace="/medium_detail_files">
		<default-interceptor-ref name="submoduleStack"/>

		<action name="view" class="com.topdev.tsp.action.modules.medium.detail.files.ViewAction" method="view">
			<interceptor-ref name="moduleStack"/>
			<result name="success">/pages/modules/medium/detail/files/view.jsp</result>
