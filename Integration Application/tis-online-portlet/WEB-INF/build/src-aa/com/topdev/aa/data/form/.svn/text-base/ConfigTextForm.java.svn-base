package com.topdev.aa.data.form;

import com.topdev.aa.data.DataObject;

public class ConfigTextForm extends DataObject
{
	private static final long	serialVersionUID	= 1L;
	private String FONTSIZE;
	private String FONTFAMILY_CODE;
	private String FONTFAMILY_DECODE;
	private String FONT_KZ_UNDERLINE;
	private String FONTWEST_CODE;
	private String FONTWEST_DECODE;
	
	public String getFONT_KZ_UNDERLINE()
	{
		return FONT_KZ_UNDERLINE;
	}
	public void setFONT_KZ_UNDERLINE(String font_kz_underline)
	{
		FONT_KZ_UNDERLINE = font_kz_underline;
	}
	public String getFONTFAMILY_CODE()
	{
		return FONTFAMILY_CODE;
	}
	public void setFONTFAMILY_CODE(String fontfamily_code)
	{
		FONTFAMILY_CODE = fontfamily_code;
	}
	public String getFONTFAMILY_DECODE()
	{
		return FONTFAMILY_DECODE;
	}
	public void setFONTFAMILY_DECODE(String fontfamily_decode)
	{
		FONTFAMILY_DECODE = fontfamily_decode;
	}
	public String getFONTSIZE()
	{
		return FONTSIZE;
	}
	public void setFONTSIZE(String fontsize)
	{
		FONTSIZE = fontsize;
	}
	public String getFONTWEST_CODE()
	{
		return FONTWEST_CODE;
	}
	public void setFONTWEST_CODE(String fontwest_code)
	{
		FONTWEST_CODE = fontwest_code;
	}
	public String getFONTWEST_DECODE()
	{
		return FONTWEST_DECODE;
	}
	public void setFONTWEST_DECODE(String fontwest_decode)
	{
		FONTWEST_DECODE = fontwest_decode;
	}
}