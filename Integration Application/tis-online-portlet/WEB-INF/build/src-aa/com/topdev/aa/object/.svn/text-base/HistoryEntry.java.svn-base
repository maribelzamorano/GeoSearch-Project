package com.topdev.aa.object;

public class HistoryEntry
{
	public	String	label=null;
	public	String	mapping=null;
	public	String	url=null;

	public HistoryEntry(String pLabel, String pMapping, String pUrl)
	{
		this.label		=pLabel;
		this.mapping	=pMapping;	
		this.url		=pUrl;	
	}

	public HistoryEntry(String pMapping, String pUrl)
	{
		this.mapping	=pMapping;	
		this.url		=pUrl;	
	}

	public	String	getLabel()									{ return (this.label); }
	public	String	getMapping()								{ return (this.mapping); }
	public	String	getUrl()									{ return (this.url); }

	public	void	setLabel(String s)							{ this.label=s; }
	public	void	setMapping(String s)						{ this.mapping=s; }
	public	void	setUrl(String s)							{ this.url=s; }
}